{
    "files.exclude": {
        "node_modules": true,
        "app/**/*.js*": true,
        "app/**/*.css": { "when": "$(basename).post.css" }
    },
    "search.exclude": {
        "typings": true
    },
    "editor.fontSize": 13,
    "editor.tabSize": 4,
    "editor.insertSpaces": true,
    "editor.renderWhitespace": true
}